{
    "name": "qobo/cakephp-notes",
    "description": "Notes plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "cakephp/cakephp": "~3.0",
        "muffin/trash": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Notes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notes\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
